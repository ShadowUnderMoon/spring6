<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="book" class="org.example.Book">
        <property name="name" value="vue3"></property>
        <property name="author" value="hanj"></property>
    </bean>

    <bean id="bookCon" class="org.example.Book">
        <constructor-arg name="name" value="java"></constructor-arg>
        <constructor-arg name="author" value="hanj"></constructor-arg>
    </bean>
</beans>